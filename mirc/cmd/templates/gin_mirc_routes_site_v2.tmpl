package v2

import (
    . "github.com/alimy/mir/v2"
    . "github.com/alimy/mir/v2/engine"
)

func init() {
    AddEntry(new(Site))
}

// Site site v2 interface info
type Site struct {
    Group    Group `mir:"v2"`
    Index    Get   `mir:"/index/"`
    Articles Get   `mir:"/articles/:category/"`
    Category Get   `mir:"/category/"`
}

