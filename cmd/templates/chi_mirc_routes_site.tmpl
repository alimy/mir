package routes

import (
    . "github.com/alimy/mir/v2"
    . "github.com/alimy/mir/v2/engine"
)

func init() {
    AddEntry(new(Site))
}

// Site site interface info
type Site struct {
    Chain    Chain `mir:"-"`
    Index    Get   `mir:"/index/"`
    Articles Get   `mir:"/articles/{category}/{id:[0-9]+}"`
}
